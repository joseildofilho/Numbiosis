                    tools:ignore="UnusedAttribute" />

                <EditText
                    android:id="@+id/x_value2"
                    android:layout_width="50dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8dp"
                    android:layout_marginStart="16dp"

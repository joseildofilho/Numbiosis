                    android:layout_marginBottom="8dp"
                    android:layout_marginStart="16dp"
                    android:backgroundTint="@color/colorDetail"
                    android:ems="10"
                    android:inputType="numberSigned|numberDecimal"
                    android:textAlignment="center"

                    android:textAppearance="@android:style/TextAppearance.WindowTitle"
                    android:textColor="@color/colorText"
                    android:textSize="24sp"
                    app:layout_constraintBottom_toTopOf="@+id/z_value3"
                    app:layout_constraintStart_toEndOf="@+id/add5"
                    app:layout_constraintTop_toBottomOf="@+id/z_value"
                    app:layout_constraintVertical_bias="0.583"
                    tools:ignore="UnusedAttribute" />

                <EditText
                    android:id="@+id/z_value3"
                    android:layout_width="50dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8dp"
                    android:layout_marginStart="16dp"
                    android:backgroundTint="@color/colorDetail"
                    android:ems="10"
                    android:inputType="numberSigned|numberDecimal"
                    android:textAlignment="center"
                    android:textAppearance="@android:style/TextAppearance.WindowTitle"
                    android:textColor="@color/colorText"
                    android:textSize="24sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/add6"
                    tools:ignore="UnusedAttribute" />

